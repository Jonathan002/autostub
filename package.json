{
  "name": "autostub",
  "version": "1.0.3",
  "description": "A watcher to automatically generate stubs for your javascript/typescript classes",
  "main": "./build/src/app.js",
  "bin": {
    "autostub": "./bin/autostub.js"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "copyProject": "cp -R angular-project-tester ./angular-project",
    "removeOldTestProject": "rm -r ./angular-project",
    "start": "npm run removeOldTestProject && npm run copyProject && ts-node src/task-fs-handler.ts",
    "pg": "ts-node src/playground.ts",
    "build": "tsc",
    "deploy:dev": "npm run build && npm link"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jonathan002/autostub.git"
  },
  "keywords": [
    "auto",
    "stub",
    "autostub",
    "testing",
    "test"
  ],
  "author": "Jonathan Fernandez",
  "license": "ISC",
  "dependencies": {
    "case": "^1.5.5",
    "commander": "^2.19.0",
    "eslint": "4.19.1",
    "fs-extra": "^7.0.0",
    "node-dir": "^0.1.17",
    "path": "^0.12.7",
    "typescript-estree": "^3.0.0",
    "typescript-parser": "2.6.1"
  },
  "devDependencies": {
    "@types/estree": "0.0.39",
    "@types/fs-extra": "^5.0.4",
    "@types/jasmine": "^2.8.9",
    "@types/node": "^10.5.6",
    "@types/node-dir": "0.0.32",
    "eslint-config-eslint": "4.0.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-release": "1.1.0",
    "jasmine": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jest": "23.1.0",
    "npm-license": "0.3.3",
    "shelljs": "0.8.2",
    "shelljs-nodecli": "0.1.1",
    "ts-node": "^7.0.1",
    "typescript": "~3.1.1"
  }
}
